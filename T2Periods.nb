     RowBox[{"\[Theta]", "^", "2"}]}], "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{"coefflist", "=", 
  RowBox[{
   RowBox[{"CoefficientList", "[", 
    RowBox[{
     RowBox[{"reducedpicardfuchs", "[", "z", "]"}], ",", "\[Theta]"}], "]"}], 
   "//", "FullSimplify"}]}], "\[IndentingNewLine]", 
 RowBox[{"PFfunctional", "=", 
  RowBox[{"Collect", "[", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"f", "[", "z", "]"}], ",", 
        RowBox[{
         RowBox[{"deriv", "[", "z", "]"}], "@", 
         RowBox[{"f", "[", "z", "]"}]}], ",", 
        RowBox[{
         RowBox[{"deriv", "[", "z", "]"}], "@", 
         RowBox[{
          RowBox[{"deriv", "[", "z", "]"}], "@", 
          RowBox[{"f", "[", "z", "]"}]}]}]}], "}"}], ".", "coefflist"}], "//",
