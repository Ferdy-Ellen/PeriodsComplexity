Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"picardfuchs", "[", "z_", "]"}], ":=", 
   RowBox[{
    RowBox[{"\[Theta]", "^", "2"}], "-", 
    RowBox[{"z", "*", 
     RowBox[{"(", 
      RowBox[{"\[Theta]", "+", "a1"}], ")"}], 
     RowBox[{"(", 
      RowBox[{"\[Theta]", "+", "a2"}], ")"}]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"reducedpicardfuchs", "[", "z_", "]"}], ":=", 
  RowBox[{
   RowBox[{"picardfuchs", "[", "z", "]"}], "/", 
   RowBox[{"Coefficient", "[", 
    RowBox[{
