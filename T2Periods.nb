Cell[CellGroupData[{

Cell["Monodromy", "Section",
 CellChangeTimes->{{3.9549100546271067`*^9, 
  3.954910055618586*^9}},ExpressionUUID->"f7a7485b-7444-4305-9fab-\
023a5389c1fe"],

Cell[TextData[{
 "Write down the monodromy matrices and the corresponding log-monodromy \
matrices. For the point at infinity, the log of the monodromy matrix will \
return a logmonodromy matrix with eigenvalues between (-\[Pi],\[Pi]) since \
Mathematica takes the principal branch. However, we want all the eigenvalues \
to be between 0 and 1 so that they correspond to ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"(", 
         RowBox[{
           SubscriptBox[
            StyleBox["a", "TI"], "1"], ",", 
           SubscriptBox[
            StyleBox["a", "TI"], "2"]}], ")"}], TraditionalForm], 
     "errors" -> {}, "input" -> "(a_1,a_2)", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "17a883a5-b216-4798-a999-52cadc60ccc7"],
 ", so we let Mathematica search a better log-monodromy matrix for us (which \
is guaranteed to exist)."
}], "Text",
 CellChangeTimes->{{3.95491006353209*^9, 3.954910076594942*^9}, {
  3.955182292175269*^9, 3.955182386326758*^9}, {3.9551839274615517`*^9, 
  3.955183939456377*^9}, {3.955184114212731*^9, 
  3.9551841189613934`*^9}},ExpressionUUID->"cabe4a88-b5fd-4791-baaf-\
bd168bfd9928"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"MonodromyLCS", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"1", ",", "1"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"0", ",", "1"}], "}"}]}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"MonodromyConif", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"1", ",", "0"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"-", "\[Kappa]"}], ",", "1"}], "}"}]}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"MonodromyInfty", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"1", "-", "\[Kappa]"}], ",", 
       RowBox[{"-", "1"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"\[Kappa]", ",", "1"}], "}"}]}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Monodromy", "=", 
   RowBox[{
    RowBox[{"MonodromyLCS", "*", 
     RowBox[{"relevantpoint", "[", 
      RowBox[{"[", "1", "]"}], "]"}]}], "+", 
    RowBox[{"MonodromyConif", "*", 
     RowBox[{"relevantpoint", "[", 
      RowBox[{"[", "2", "]"}], "]"}]}], "+", 
    RowBox[{"MonodromyInfty", "*", 
     RowBox[{"relevantpoint", "[", 
      RowBox[{"[", "3", "]"}], "]"}]}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"LogMonodromyLCS", "=", 
   RowBox[{
    FractionBox["1", 
     RowBox[{"2", "Pi", " ", "I"}]], 
    RowBox[{"MatrixLog", "[", "MonodromyLCS", "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"LogMonodromyConif", "=", 
   RowBox[{
    FractionBox["1", 
     RowBox[{"2", "Pi", " ", "I"}]], 
    RowBox[{"MatrixLog", "[", "MonodromyConif", "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"LogMonodromyInfty", "=", 
   RowBox[{
    FractionBox["1", 
     RowBox[{"2", "Pi", " ", "I"}]], 
    RowBox[{"MatrixLog", "[", "MonodromyInfty", "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"JordanDecomposition", "[", "LogMonodromyInfty", "]"}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"CalculatedLogMonodromyInfty", "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"{", 
         RowBox[{"t1", ",", "t2", ",", "t3", ",", "t4"}], "}"}], "/.", 
        RowBox[{"FindInstance", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"Eigenvalues", "[", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"{", 
                RowBox[{"t1", ",", "t2"}], "}"}], ",", 
               RowBox[{"{", 
                RowBox[{"t3", ",", "t4"}], "}"}]}], "}"}], "]"}], "==", 
            RowBox[{"{", 
             RowBox[{"a1", ",", "a2"}], "}"}]}], "&&", 
           RowBox[{
            RowBox[{"MatrixExp", "[", 
             RowBox[{"2", "Pi", " ", "I", "*", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{"{", 
                 RowBox[{"t1", ",", "t2"}], "}"}], ",", 
                RowBox[{"{", 
                 RowBox[{"t3", ",", "t4"}], "}"}]}], "}"}]}], "]"}], "==", 
            "Monodromy"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"t1", ",", "t2", ",", "t3", ",", "t4"}], "}"}]}], "]"}]}], 
       "//", 
       RowBox[{"Flatten", 
        RowBox[{"(*", " ", 
         RowBox[{
         "ugly", " ", "way", " ", "of", " ", "doing", " ", "it", " ", "but", 
          " ", "dont", " ", "know", " ", "how", " ", "to", " ", "improve", 
          " ", "right", " ", "now"}], " ", "*)"}], " ", "\[IndentingNewLine]",
         "LogMonodromyInfty"}]}], "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"CalculatedLogMonodromyInfty", "[", 
           RowBox[{"[", "1", "]"}], "]"}], ",", 
          RowBox[{"CalculatedLogMonodromyInfty", "[", 
           RowBox[{"[", "2", "]"}], "]"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"CalculatedLogMonodromyInfty", "[", 
           RowBox[{"[", "3", "]"}], "]"}], ",", 
          RowBox[{"CalculatedLogMonodromyInfty", "[", 
           RowBox[{"[", "4", "]"}], "]"}]}], "}"}]}], "}"}]}]}], ";"}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{"LogMonodromy", "=", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"relevantpoint", "[", 
      RowBox[{"[", "1", "]"}], "]"}], "*", "LogMonodromyLCS"}], "+", 
    RowBox[{
     RowBox[{"relevantpoint", "[", 
      RowBox[{"[", "2", "]"}], "]"}], "*", "LogMonodromyConif"}], "+", 
    RowBox[{
     RowBox[{"relevantpoint", "[", 
      RowBox[{"[", "3", "]"}], "]"}], "*", "LogMonodromyInfty"}]}], "//", 
   "FullSimplify"}]}]}], "Input",
 CellChangeTimes->{{3.954911040816121*^9, 3.9549110421175003`*^9}, {
